<link rel='stylesheet' type='text/css' href='style.css'>
<link rel='stylesheet' type='text/css' href='/style_from_glpi.css'>
<link href="/favicon.ico" type="images/x-icon" rel="shortcut icon">
<script type="text/javascript" src="../script.js"></script>
<script type="text/javascript">if ("%err%" != "") alert("Ошибка","%err%");</script>


<head>
	<title>
		СКУД
	</title>
</head>
<body class=" ext-gecko ext-gecko3">
<div id='header'>
	<div id='c_logo'><a href='/'> </a></div>
	<div id='c_preference'><a href='http://comm.gtptmn.gtp.transneft.ru/'>Главная</a> <a style='background-color:rgb(221,255,112);' href='http://comm.gtptmn.gtp.transneft.ru/update/index.php'>CКУД</a> <a href='http://comm.gtptmn.gtp.transneft.ru/urv/'>УРВ</a> <a href='http://comm.gtptmn.gtp.transneft.ru/tel/'>Телефония</a> <a href='http://comm.gtptmn.gtp.transneft.ru/graphic/'>ОИТ</a></div>
	<div id="c_recherche">
		 
		<div id="boutonRecherche"> </div>
		<div id="champRecherche"> </div> 
		<div class="sep"> </div>
	</div>
	<div id='c_menu'>
		<ul id='menu'>
			%menu%
		</ul>
	</div>
	<div id='c_ssmenu1'></div>
</div>
<div id='page'>
	<table class='tab_cadre_pager'>
		<tr>
			<td class='tab_bg_2'>%header%</td>
		</tr>
	</table>	
	<br>
        %add%
    <br>
        %log%
	<div class='center'>%centerbar%
        <div class='wait' id='wait_user' name='wait_user' style='visibility:%wait_user%;'>
            <input type='hidden' value='0' name='id_user' id='id_user' />
            <input type='hidden' id='w_i_new' name='w_i_new' value='' />
            <table style='border:1px solid gray;z-index:1000;margin:10px 0px 10px 10px;width:92%;height:100%;'>
                <tr>
                    <td class='tab_bg_2'>
                        <img id='foto' name='foto' src='%domain%/photos/none.png' style='height:150px;' value='' >
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='text' id='w_f_new' name='w_f_new' style='width:90%;height:90%;' value=''>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='text' id='w_n_new' name='w_n_new' style='width:90%;height:90%;' value=''>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='text' id='w_p_new' name='w_p_new' style='width:90%;height:90%;' value=''>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='text' id='w_l_new' name='w_l_new' style='width:90%;height:90%;' value=''>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input list='posts' type='text' id='w_post_new' name='w_post_new' style='width:90%;height:90%;' value='' />
                        <datalist id='posts'>
                            %posts%
                        </datalist>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input list='otdels' type='text' id='w_otdel_new' name='w_otdel_new' style='width:90%;height:90%;' value='' />
                        <datalist id='otdels'>
                            %otdels%
                        </datalist>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='checkbox' id='w_fired' name='w_fired' value='1' /> <span style='font-size:9px;'>Уволен</span>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='button' value='Очистить' onclick='usrTIMEjs(0, 4,"%domain%");'  style='width:150px;height:25px;'>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='button' value='Сохранить' onclick='usrTIMEjs(id_new.value, 2,"%domain%");' style='width:150px;height:25px;'><br>
                    </td>
                </tr>
                <tr>
                    <td class='tab_bg_2'>
                        <input type='button' value='Удалить' onclick='usrTIMEjs(id_new.value, 3,"%domain%");'  style='width:150px;height:25px;'>
                    </td>
                </tr>
            </table>
        </div>
        <div class='wait' id='wait_time' name='wait_time' style='width:230px;visibility:hidden;'>
                <input type='hidden' value='0' name='id_time' id='id_time'/>
                <input type='hidden' value='' name='d' id='d'/>
                <input type='hidden' value='' name='m' id='m'/>
                <input type='hidden' value='' name='y' id='y'/>
                <table style='border:1px solid gray;z-index:1000;margin:10px 0px 10px 10px;width:90%;height:100%;'>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='text' id='fio' name='fio' style='width:80%;height:16px;' value='ФИО' disabled>
                        </td></tr>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='date' id='date' name='date' style='width:90%;height:16px;' value='2012-12-20' />
                        </td></tr>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='time' id='in_time' name='in' style='width:40%;height:16px;' value='00:00' />
                            <input type='time' id='out_time' name='out' style='width:40%;height:16px;' value='23:59' />
                        </td></tr>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='text' id='time' name='in_time' style='width:40%;height:16px;' value='0' />
                            <input type='text' id='m_time' name='m_time' style='width:40%;height:16px;' value='0' />
                        </td></tr>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='button' value='Сохранить' onclick='usrTIMEjs(id.value,6);' style='width:150px;height:25px;' >
                        </td></tr>
                    <tr class='tab_bg_1'>
                        <td class='tab_bg_2'>
                            <input type='button' value='Удалить' onclick='usrTIMEjs(id.value,7);' style='width:150px;height:25px;' >
                        </td></tr>
                </table>
        </div>"
    </div>
</div>
<div id="footer">
	<table width="100%">
		<tr>
			<td class="left"><span class="copyright">%version%</span></td>
			<td class="right"><a href="%domain%"><span class="copyright">%footer%</span></a></td>
		</tr>
	</table>
</div>
</body>
</html>